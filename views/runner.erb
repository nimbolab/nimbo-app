<!DOCTYPE html>
<html>
  <head>
    <title>Nimbo suite</title>
  </head>
  <body>
    <script src="/javascripts/jasmine.js"></script>
    <% @client_scripts.each do |script| %>
      <script src="<%= "/suites/#{@suite_id}/#{script}" %>"></script>
    <% end %>
    <script src="/javascripts/jasmine.NimboReporter.js"></script>
      <script>
          window.onload = function () {
              jasmine.getEnv().addReporter(new jasmine.NimboReporter());
              jasmine.getEnv().execute();
          }
      </script>
  </body>
</html>
